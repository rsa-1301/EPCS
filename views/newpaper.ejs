                <div class="col-sm-12">
                    <h1><b><center>Upload a new paper</center></b></h1><br><br>
                    <form method="post" action="/newpaper" encType="multipart/form-data">         
                        <div class="form-group col-sm-3"></div>
                        Enter Course Code
                        <div class="row">
                                <div class="form-group col-sm-3"></div>
                            
                        <div class="form-group col-sm-6">
                            
                            <input type="text" class="form-control" name="code">
                        </div>
                            <br>
                            <br>
                            <br>
                        
                            <div class="form-group col-sm-3"></div>
                         Enter Year
                        <div class="row">	
                                <div class="form-group col-sm-3"></div>
                        <div class="form-group col-sm-6">
                                <input type="text" class="form-control"  name="year" required/>
							</div>
                            </div>
                            <br>
                            
                            <div class="row">							
                                    <div class="form-group col-sm-3"></div>
                            <div class="form-group col-sm-6">
                            Select Semester
                            <select name="sem" class="form-control">
                            <option class="form-control" value="FALLSEM" name="FALLSEM">Fall Semester</option>
                            <option class="form-control" value="WINSEM" name="WINSEM">Winter Semester</option>
                            </select>
                            </div>
                            </div>
                            <br>
                            <div class="row">						
                                    <div class="form-group col-sm-3"></div>
                            <div class="form-group col-sm-6">    
                                
                            Select Exam Type
                            <select class="form-control" name="type">
                            <option value="CAT1" name="CAT1">CAT 1</option>
                            <option value="CAT2" name="CAT2">CAT 2</option>
                            <option value="FAT" name="FAT">FAT</option>
                            </select>
                            </div>
                            </div>
                            <br>
                            <div class="row">							
                                    <div class="form-group col-sm-3"></div>
                            <div class="form-group col-sm-6">
                            
                            Select Slot 
                            <select class="form-control" name="slot">
                            <option class="form-control" value="A1" name="A1">A1</option>
                            <option class="form-control" value="A2" name="A2">A2</option>
                            <option class="form-control" value="B1" name="B1">B1</option>
                            <option class="form-control" value="B2" name="B2">B2</option>
                            <option class="form-control" value="C1" name="C1">C1</option>
                            <option class="form-control" value="C2" name="C2">C2</option>
                            <option class="form-control" value="D1" name="D1">D1</option>
                            <option class="form-control" value="D2" name="D2">D2</option>
                            </select>
                            </div>
                            </div>
                        <br>
                            <div class="row">							
                                    <div class="form-group col-sm-3"></div>
                            <div class="form-group col-sm-6">
                            
                            Choose file to upload : 
                            <input type="file" class="form-control" name="imgUploader" required>
                            <br>
                                <br>
                                <center>
                                <button class="btn">Submit</button></center>
                            </div>
                            </div>
                        </form>
                        </div>
                        </div>
                        </div>
